        </record>

        <record id="fiscal_position_contribes" model="account.fiscal.position">
            <field name="name">Contribuyente Eventual Social</field>
            <field name="afip_code">14</field>
            <field name="denomination_id" ref="denomination_B"/>
            <field name="denom_supplier_id" ref="denomination_C"/>
        </record>

    </data>

</openerp>

