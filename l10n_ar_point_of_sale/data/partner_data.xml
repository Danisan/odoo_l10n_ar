<openerp>

    <data noupdate="1">

        <record id="denomination_A" model="invoice.denomination">
            <field name="name">A</field>
            <field name="desc">Denominacion A</field>
            <field name="vat_discriminated">1</field>
        </record>

        <record id="denomination_B" model="invoice.denomination">
            <field name="name">B</field>
            <field name="desc">Denominacion B</field>
        </record>

        <record id="denomination_C" model="invoice.denomination">
            <field name="name">C</field>
            <field name="desc">Denominacion C</field>
        </record>

        <record id="denomination_M" model="invoice.denomination">
            <field name="name">M</field>
            <field name="desc">Denominacion M</field>
        </record>

        <record id="denomination_E" model="invoice.denomination">
            <field name="name">E</field>
            <field name="desc">Denominacion E</field>
        </record>

        <record id="fiscal_position_ri" model="account.fiscal.position">
            <field name="name">Responsable Inscripto</field>
            <field name="afip_code">01</field>
            <field name="denomination_id" ref="denomination_A"/>
            <field name="denom_supplier_id" ref="denomination_A"/>
        </record>

        <record id="fiscal_position_rni" model="account.fiscal.position">
            <field name="name">Responsable No Inscripto</field>
            <field name="afip_code">02</field>
            <field name="denomination_id" ref="denomination_B"/>
            <field name="denom_supplier_id" ref="denomination_C"/>
        </record>

        <record id="fiscal_position_nr" model="account.fiscal.position">
            <field name="name">No Responsable</field>
            <field name="afip_code">03</field>
            <field name="denomination_id" ref="denomination_B"/>
            <field name="denom_supplier_id" ref="denomination_C"/>
        </record>

        <record id="fiscal_position_exempt" model="account.fiscal.position">
            <field name="name">Exento</field>
            <field name="afip_code">04</field>
            <field name="denomination_id" ref="denomination_B"/>
            <field name="denom_supplier_id" ref="denomination_C"/>
        </record>

        <record id="fiscal_position_cf" model="account.fiscal.position">
            <field name="name">Consumidor Final</field>
            <field name="afip_code">05</field>
            <field name="denomination_id" ref="denomination_B"/>
            <field name="denom_supplier_id" ref="denomination_C"/>
