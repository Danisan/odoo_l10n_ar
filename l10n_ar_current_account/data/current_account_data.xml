<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="imputation_journal_sequence" model="ir.sequence">
            <field name="name">Secuencia de diario de imputaciones</field>
            <field eval="4" name="padding"/>
            <field name="prefix">IMP-%(day)s/%(month)s/%(year)s/</field>
        </record>

        <record id="imputation_journal" model="account.journal">
            <field name="name">Diario de imputaciones</field>
            <field name="code">IMP</field>
            <field name="type">general</field>
            <field name="sequence_id" ref="imputation_journal_sequence"/>
        </record>

    </data>
</openerp>
